{
  "name": "d3-sheet-music",
  "version": "0.0.0-semantic-released",
  "description": "Sheet music with D3.js",
  "main": "src/index.js",
  "scripts": {
    "commit": "git-cz",
    "check-coverage": "istanbul check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "test": "mocha src/index.test.js -w",
    "test:single": "istanbul cover -x *.test.js _mocha -- -R spec src/index.test.js",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nlarche/d3-sheet-music.git"
  },
  "keywords": [
    "d3",
    "sheet",
    "music"
  ],
  "author": "Nicolas LARCHE <nico060984@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nlarche/d3-sheet-music/issues"
  },
  "homepage": "https://github.com/nlarche/d3-sheet-music#readme",
  "dependencies": {
    "d3": "3.5.6",
    "extend": "3.0.0"
  },
  "devDependencies": {
    "chai": "3.3.0",
    "commitizen": "1.0.5",
    "cz-conventional-changelog": "1.1.2",
    "ghooks": "0.3.2",
    "install": "0.1.8",
    "istanbul": "0.3.21",
    "jsdom": "6.5.1",
    "mocha": "2.3.3",
    "mocha-jsdom": "1.0.0",
    "semantic-release": "^4.3.5"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test:single && npm run check-coverage"
    }
  }
}